{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "jest",
    "start": "sls offline start --stage dev --env local",
    "lint": "tslint './src/**/*.{ts,tsx}'",
    "ts": "tsc --noEmit --pretty --noUnusedParameters --noUnusedLocals",
    "deploy": "sls deploy --env production",
    "docker:up": "cd ../ && docker-compose up",
    "docker:down": "cd ../ && docker-compose down",
    "ssh": "docker exec -it $CONTAINER sh",
    "codegen": "graphql-codegen --config codegen.yml",
    "codechecks": "yarn ts && yarn lint && yarn test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/add": "^1.0.7",
    "@graphql-codegen/cli": "^1.0.7",
    "@graphql-codegen/fragment-matcher": "1.0.7",
    "@graphql-codegen/introspection": "^1.0.7",
    "@graphql-codegen/typescript": "1.12.1",
    "@graphql-codegen/typescript-resolvers": "1.0.7",
    "@types/aws-lambda": "^8.10.17",
    "@types/cookie": "^0.3.3",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^11.13.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^5.3.1",
    "source-map-support": "^0.5.10",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.0.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@octokit/plugin-throttling": "^2.4.0",
    "@octokit/rest": "^16.25.1",
    "apollo-server-lambda": "^2.9.16",
    "aws-sdk": "^2.606.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.9.1",
    "cookie": "^0.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "global": "^4.4.0",
    "gql": "^1.1.2",
    "graphql": "^14.2.1",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.3",
    "merge-graphql-schemas": "^1.5.8",
    "njwt": "^1.0.0",
    "rand-token": "^0.4.0",
    "serverless": "^1.51.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.29.6",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(spec|test).+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/.jest/setEnvVars.ts"
    ]
  }
}
